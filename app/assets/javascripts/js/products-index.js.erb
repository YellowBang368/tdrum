$(document).ready(function(){
  var $products = $(".content")
  $products.hide();
  $(".content#all-products").show();
  $(".filter a").click(function(){
    var $this = $(this);
    var $contentId = $this.attr("data");
    $(".filter a").removeClass("active");
    $this.addClass("active");
    $(".content").hide();
    $("#"+$contentId).show();
  });
});
